proof
  let y be Element of Y;
  B1: dom g = Y & dom (f * g) = dom g by FUNCT_2:123, FUNCT_2:def 1;
  y = f.(g.y) by A3
   .= (f * g).y by B1, FUNCT_1:12;
  hence (f * g).y = y;
