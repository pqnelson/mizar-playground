        then A8: a*H = U by A2, GROUP_2:115, XBOOLE_0:3;
        x0 in x0*H & x0 in U by A5, GROUP_2:108;
        then x0 in x0*H /\ U by XBOOLE_0:def 4;
        hence thesis by A4,A6,A7,A8,XBOOLE_0:def 7;
