theorem Th16a:
  for G being Group
  for H being strict Subgroup of G
  holds (H is normal) iff (for f being inner Automorphism of G holds (f .: H) = H)
proof
  let G be Group;
  let H be strict Subgroup of G;
   C1: (H is normal) implies (for f being inner Automorphism of G holds
   (f .: H) = H)
  proof
    assume A0: H is normal;
    let f be inner Automorphism of G;
    consider a being Element of G such that
    A1: for x being Element of G holds f.x = x |^ a
    by Def5;
    Image(f|H) = H |^ a by A1,Th7;
    then A2: f .: H = H |^ a by GRSOLV_1:def 3;
    H |^ a = the multMagma of H by A0,GROUP_3:def 13
    .= H by GROUP_2:def 6;
    then f .: H = H by A2;
    hence thesis;
  end;
  C2: not ((for f being inner Automorphism of G holds (f .: H = H)) implies H is normal)
      implies contradiction
  proof
    assume B1: for f being inner Automorphism of G holds f .: H = H;
    assume not H is normal;
    then consider a being Element of G such that
    B2: H |^ a <> the multMagma of H
    by GROUP_3:def 13;
    consider f being inner Automorphism of G such that
    B3: (for x being Element of G holds f.x= x |^ a)
    by ThAlwaysFindInnerAuto;
    Image(f|H) = H |^ a by B3, Th7;
    then f .: H = H |^ a by GRSOLV_1:def 3;
    hence contradiction by B1,B2;
  end;

  thus thesis by C1,C2;
end;

Lm25:
