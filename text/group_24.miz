      .= (x.((nat_hom V).g))*(x.a) by A3,GROUP_6:def 8
      .= g*(x.a) by Z2;
      (x.(g * V))*(x.a) = (x.((nat_hom V).g))*(x.a) by A3,GROUP_6:def 8
